#. type: Title ###
#: ../src/guide/views/asset.md
#, no-wrap
msgid "Basic Asset Bundle"
msgstr ""

#. type: Plain text
#: ../src/guide/views/asset.md
msgid "Here's a simple asset bundle definition:"
msgstr ""

#. type: Fenced code block (php)
#: ../src/guide/views/asset.md
#, no-wrap
msgid ""
"<?php\n"
"\n"
"declare(strict_types=1);\n"
"\n"
"namespace App\\Asset;\n"
"\n"
"use Yiisoft\\Assets\\AssetBundle;\n"
"\n"
"final class MainAsset extends AssetBundle\n"
"{\n"
"    public string $basePath = '@assets';\n"
"    public string $baseUrl = '@assetsUrl';\n"
"    \n"
"    public array $css = [\n"
"        'css/main.css',\n"
"        'css/responsive.css',\n"
"    ];\n"
"    \n"
"    public array $js = [\n"
"        'js/main.js',\n"
"        'js/utils.js',\n"
"    ];\n"
"    \n"
"    public array $depends = [\n"
"        BootstrapAsset::class,\n"
"        JqueryAsset::class,\n"
"    ];\n"
"}\n"
msgstr ""

#. type: Title ###
#: ../src/guide/views/asset.md
#, no-wrap
msgid "Asset Bundle Properties"
msgstr ""

#. type: Plain text
#: ../src/guide/views/asset.md
#, no-wrap
msgid ""
"**Path Configuration:**\n"
"- `$basePath` - Physical path where asset files are located\n"
"- `$baseUrl` - Web-accessible URL path for assets\n"
"- `$sourcePath` - Source directory for assets that need to be published\n"
msgstr ""

#. type: Plain text
#: ../src/guide/views/asset.md
#, no-wrap
msgid ""
"**Asset Files:**\n"
"- `$css` - Array of CSS files\n"
"- `$js` - Array of JavaScript files  \n"
msgstr ""

#. type: Plain text
#: ../src/guide/views/asset.md
#, no-wrap
msgid ""
"**Dependencies:**\n"
"- `$depends` - Array of other asset bundles this bundle depends on\n"
msgstr ""

#. type: Plain text
#: ../src/guide/views/asset.md
#, no-wrap
msgid ""
"**Options:**\n"
"- `$jsOptions` - HTML attributes for JavaScript tags\n"
"- `$cssOptions` - HTML attributes for CSS link tags\n"
msgstr ""

